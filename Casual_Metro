Esp:CreateParagraph({Title = "Новое ESP", Content = "Окуп ESP использовать только по отдельности!"})

Esp:CreateToggle({
	Name = "Окуп Одежда [OPTIMIZED]",
	CurrentValue = false,
	Callback = function(value)
		if value then 
			_G.ESP_Epic = true
			while _G.ESP_Epic do
				task.spawn(espEnable(whatBuyListS, whatBuyListP, Color3.fromRGB(50, 255, 255)))
				task.wait(1)
			end
		else
			_G.ESP_Epic = false
			destroyEsp()
		end 
	end    
})

